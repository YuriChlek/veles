{
    "compilerOptions": {
        "target": "ES6",
        "module": "commonjs",
        "rootDir": "../",
        "outDir": "./dist",
        "baseUrl": "../",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        /*"paths": {
            "@api*//*": [
                ".*//*"
            ]
        },*/
        "noEmit": true
    },
    "include": ["**/*.ts"],
    "exclude": ["node_modules"]
}